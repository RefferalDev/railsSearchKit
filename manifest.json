{
  "name": "Ruby on Rails Search Kit",
  "description": "One-keystroke access to the search bars every Rails developer needs",
  "version": "0.2.4",
  "browser_action": {
    "default_title": "Ruby on Rails Search Kit",
    "default_popup": "search_bars.html",
    "default_icon": "images/popup-icon.png"
  },
  "icons": {
    "128": "images/128-icon.png"
  },
   "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  },
  "manifest_version": 2
}
